{{page-title "Смены"}}
<div class="row list">
  <div class="col s12">
    <div class="card">
      <div class="card-content schedule-container">
        <span class="card-title">Расписание</span>
        {{schedule refreshTrigger=refreshTrigger parentRoute=parentRoute newClick=(action 'newWorkshift') read=read}}
      </div>
    </div>
  </div>
  <div class="col s12">
    <div class="card">
      <div class="card-content">
        <span class="card-title">Мои смены</span>
        {{list-table 
          modelTable=model 
          rows=rows
          parentRoute=parentRoute
          modelTitle="Смена"
          refreshTrigger=refreshTrigger
          btnAddVisible=false
        }}
      </div>
    </div>
  </div>
</div>
{{outlet}}